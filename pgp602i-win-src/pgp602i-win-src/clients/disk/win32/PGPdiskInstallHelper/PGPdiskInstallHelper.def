LIBRARY		PGPdskIH
DESCRIPTION	'PGPdisk Install Helper'

EXPORTS
	KillPGPdiskResident		@1 PRIVATE		; Kill PGPdiskResident.exe.
	PGPdiskCleanup			@2 PRIVATE		; Cleanup PGPdisk reg entries.
	AreAnyPGPdisksMounted	@3 PRIVATE		; Are any PGPdisks mounted?
	IsVolumeAPGPdisk		@4 PRIVATE		; Is the drive a PGPdisk?
	UnmountAllPGPdisks		@5 PRIVATE		; Unmount all PGPdisks.
