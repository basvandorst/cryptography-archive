% Goto begining of next "line" (We go downwards)
/nl
{
 /y y 2 hbs mul sub def
 /x LM def
} def

% Next page
/nextpage
{
 showpage
 /y TM def
 /x LM def
} def

% A black hbs * hbs sized box at x y
/box
{
 newpath
  x y moveto
  hbs 0 rlineto
  0 hbs rlineto
  hbs neg 0 rlineto
 closepath fill
}def

% Blocks
/A		% hor 1
{
 box
 /x x hbs add def
 box
 /x x hbs add def
} def

/B		% hor 2
{
 /y y hbs add def
 box
 /x x hbs add def
 box
 /x x hbs add def
 /y y hbs sub def
} def

/C		% ver 1
{
