atLN	= ln -sHARDLN	= lnTAIL	= tailSED	= sedLINT	= lint5LINT	= lintMKDIR	= mkdirINSTALL	= installINSTALLFLAGS = -cBASENAME= basenameDIRNAME	= dirnameMKDEP	= $(LDAPSRC)/build/mkdep -c gcc -s -f Make-templatePWD	= pwdDATE	= dateHOSTNAME= hostname## empty target used to force rules to be invoked#FORCE: