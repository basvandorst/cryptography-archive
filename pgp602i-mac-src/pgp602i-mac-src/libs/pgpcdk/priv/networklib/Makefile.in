## priv/networklib/## $Id: Makefile.in,v 1.4 1998/08/05 05:36:56 heller Exp $#.SUFFIXES: .cpp .cp.cpp.o:	g++ -c -fhandle-exceptions $(CFLAGS) $<LDAPDIR= $(top_srcdir)/../network/ldaplibLIBLBERDIR= $(LDAPDIR)/liblberLIBLDAPDIR= $(LDAPDIR)/libldapSUBDIRS= pgpsockets keyserver tlsLOCALDEFINES= -DPROTOTYPES -D_REENTRANTLOCALINCLUDES= -I./pgpsockets/pub -I$(LDAPDIR)/includeLIBDONE= DONE $(LIBLBERDIR)/DONE \	$(LIBLDAPDIR)/DONE keyserver/DONE pgpsockets/LIBDONE tls/DONELIBTDEPS= LIBDONELIBTARGET= $(top_srcdir)/unix/libPGPsdkNetwork.aINSTALLLIBS= $(LIBTARGET)all depend clean::	@for i in $(LDAPDIR); do \		echo Making $@ in $(SUB)$$i; \		(cd $$i; $(MAKE) $(MFLAGS) SUB=$(SUB)$$i/ \			$@) || exit 1; \	doneall:: LIBDONE $(LIBTARGET)clean::	rm -f $(LIBTARGET)