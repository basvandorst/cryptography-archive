# 1st define the main's parameters
smain
name:rogue
sleep:120
emain


# Now the sign job.
# To monitor everything set run=1 and specify 
#   patterns in config file, not unique files.
#sjob sign
#name:sign-job
#sleep:30
#cfg:input/cfg_sign
#oseen:input/old_sign_list
#nseen:output/new_sign_list
#sig:md5
#run:3
#ejob

# Define the time job
#sjob time
#name:time-job
#sleep:90
#cfg:input/cfg_time
#oseen:input/old_time_list
#nseen:output/new_time_list
#run:4
#ejob

sjob perm
name:perm-job
sleep:90
cfg:input/cfg_perm
out:output/new_perm_report
ejob

#sjob own
#name:own-job
#sleep:180
#cfg:input/cfg_own
#out:output/new_own_report
#ejob

# Note that more than one job for type can be present.
