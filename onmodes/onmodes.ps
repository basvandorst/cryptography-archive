10 dict begin
	/y 0 def /x 0 def
	/rmoveto {y add /y exch def x add /x exch def} def
	/rlineto {rmoveto} def
	/show {stringwidth rmoveto} def
	/showsuper {pop stringwidth rmoveto} def
	dup exec
	x 2 div neg  y 2 div neg
end
rmoveto exec
} def

