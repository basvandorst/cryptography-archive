    <FDW  0.0 pt>
    <FSeparation 0>
   > # end of Font
   <String `delete'>
   <Font 
    <FTag `'>
    <FFamily `Times'>
    <FVar `Regular'>
    <FWeight `Regular'>
    <FAngle `Regular'>
